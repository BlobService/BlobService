<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <Description>BlobService.Core component</Description>
    <Version>0.1.0</Version>
    <Copyright>Copyright Aram Kocharyan</Copyright>
    <PackageProjectUrl>https://github.com/BlobService/BlobService</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/BlobService/BlobService/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/26712995</PackageIconUrl>
    <RepositoryUrl>https://github.com/BlobService/BlobService</RepositoryUrl>
    <PackageTags>blobservice, blobs, files, netcore</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="6.0.2" />
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="2.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="1.1.2" />
  </ItemGroup>

</Project>